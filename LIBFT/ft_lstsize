#include "libft.h"

int ft_lstsize(t_list *lst)
{
    t_list *tmpnode;
    int i;
    
    tmpnode = lst;
    i = 0;
    while (tmpnode)
    {
        tmpnode = tmpnode->next;
        i++;
    }
    return (i);
}

